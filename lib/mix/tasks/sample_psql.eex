CREATE USER commonspub_db WITH ENCRYPTED PASSWORD '<%= dbpass %>' CREATEDB;
-- in case someone runs this second time accidentally
ALTER USER commonspub_db WITH ENCRYPTED PASSWORD '<%= dbpass %>' CREATEDB;
CREATE DATABASE commonspub_db;
ALTER DATABASE commonspub_db OWNER TO commonspub_db;
\c commonspub_db;
